<div id="<%= dom_id match %>">
  <p>
    <strong>Place:</strong>
    <%= match.place_id %>
  </p>

  <p>
    <strong>Group:</strong>
    <%= match.group_id %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= match.date %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= match.notes %>
  </p>

  <p>
    <strong>Winner:</strong>
    <%= match.winner %>
  </p>

  <p>
    <strong>Team1 score:</strong>
    <%= match.team1_score %>
  </p>

  <p>
    <strong>Team2 score:</strong>
    <%= match.team2_score %>
  </p>

</div>
